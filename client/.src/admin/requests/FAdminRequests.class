' Gambas class file

Private form_request_providers As FRequestProviders
Private form_request_names As FRequestNames
Private form_request_synonyms As FRequestSynonyms
Private cTabRequestNames As Integer = 1

Public Sub Init()
   
   form_request_providers = New FRequestProviders(Vbox_RequestProviders)
   
End

Public Sub Settings_Save()
   
   form_request_providers.Settings_Save()
   form_request_names.Settings_Save()
   form_request_synonyms.Settings_Save()

End

Public Sub TabStrip1_Click()
   
   Select Case Last.index
      Case cTabRequestNames 
         If IsNull(form_request_names) Then
            With form_request_names = New FRequestNames(Vbox_Requests)
               .Parent_Form_Set_Name(Me.name)
               .Init()
             End With
         Endif 
   End Select
   
End
