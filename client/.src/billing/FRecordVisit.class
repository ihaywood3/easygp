         End Select
      Case cTab_Batching
         form_batching.Claim_Display(columnview99.Item.key) 
      Case cTab_Reports
         ' form_reports.Report_Show(Report_Titles[columnview99.Item.key], DateChooser2.value)
         DateChooser2_Activate()
   End Select
   
End

