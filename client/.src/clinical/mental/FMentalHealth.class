Public Sub Decision_support(section As String)

   Dim anger As String = ""

   "<HTML>"
   "<BODY>"
   "<P><STRONG>Presenting Complaints </STRONG></P>"
   "<UL>"
   "<LI><P>May present initially with tension-related physical symptoms"
