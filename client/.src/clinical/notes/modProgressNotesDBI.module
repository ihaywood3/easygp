Public Function ProgressNotes_Codes_Get_For_Patient(fk_patient As Integer) As Collection
   
  Dim sql As String
  
  sql = "Select * from clin_Consult.vwProgressNotesCodes where fk_patient = " & fk_patient & " order by fk_progressnote" 
  Return modDBConnect.exec_query_collection(sql)
   
End

 