   fk_lu_visit_type = currentconsult!workcover_claims[fk_visit]!fk_lu_visit_type
   visit!visit_type = currentconsult!workcover_claims[fk_visit]!visit_type
   visit!diagnosis = currentconsult!workcover_claims[fk_visit]!diagnosis
