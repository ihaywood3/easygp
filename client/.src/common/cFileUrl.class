' Gambas class file

Inherits CUrl

Private path As String

Public Sub _new(path1 As String)
   
  path = path1 
   
End

Public Sub PutFile(local As String, fname As String)
   
  Copy local To path &/ fname 
   
End

Public Sub GetFile(fname As String) As String
   
  Return path &/ fname 
   
End
