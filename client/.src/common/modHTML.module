Public Function Help_HTML_Get(topic As String) As String 
   
   Dim shtml As String
   
    Try sHtml = File.Load(modUtil.Find_File("help" &/ topic))
    sHtml = Replace(sHtml, "src=\"", "src=\"" & modUtil.Find_File("help")) 
    Return shtml
End
