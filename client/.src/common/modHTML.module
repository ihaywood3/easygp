Public Function Patient_Practice_Heading_Simple_HTML(consult As Cconsult, Optional bSmallFont As Boolean = False) As String
   '---------------------------------------------------------------------Notes_Editor.teNotes.RichText-------------------
   'Return html string with simple heading consisting on the left of column of
   'patients details/contacts, in the middle the header column on right the practice details
   '----------------------------------------------------------------------------------------
   Dim shtml As String
   Dim comm As Collection
   Dim sPhoneOffice As String
   Dim sContactsPatient As String
   
   shtml = ""
   "<table border=%BORDER% cellpadding=0 cellspacing=%cellspacing% width=100% >"
   "<TR VALIGN=TOP>"
   "<TD>"
   "%patient"
   "<BR>"
   "</TD>"
   "<TD>"
   "<P ALIGN=CENTER> %central_heading% </P><BR>"
   "</TD>"
   "<TD>"
   "%practice<BR>"
   "</TD>"
   "</TR>" 
   "</table><HR>"
   For Each comm In modDBConnect.currentUser!comms
      If comm!fk_type = const.CommModalityPhone Then
         sPhoneOffice = comm!value 
      Endif
      If comm!fk_type = const.CommModalityMobile Then
         If sPhoneOffice <> "" Then 
            sPhoneOffice = comm!value & "<BR>"
         Else
            sPhoneOffice = comm!value
         End If   
      Endif
   Next
   For Each comm In consult!comms
      If comm!fk_type = const.CommModalityPhone Or comm!fk_type = const.CommModalityMobile Or comm!fk_type = const.CommModalityHome Then    
             sContactsPatient &= const.CommModalityTextBrief[comm!fk_type] & ":" & comm!value & "<BR>"
      End If   
   Next
   shtml = Replace(shtml, "%patient", consult!patient!firstname & " " & consult!patient!surname & "<BR>" & consult!patient!street1 & "<BR>" & consult!patient!town & " " & consult!patient!postcode & "<BR>" & sContactsPatient)
   sHtml = Replace(shtml, "%practice", modDBConnect.currentUser!organisation & "<BR>" & modDBConnect.currentUser!street1 & "<BR>" & modDBConnect.currentUser!town & " " & modDBConnect.currentUser!postcode & "<BR>Phone:" & sPhoneOffice)
   Return shtml
   
End
