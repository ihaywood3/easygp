' Gambas class file

' ' Gambas class file
' CE Coded element    
' 
' identifier(ID)
' text(ST) 
' name Of coding system(ST) 
' alternate identifier(ID) 
' alternate text(ST) 
' name Of alternate coding system(ST) 
    
Public Identifier As String
Public Text As String
Public NameOfCodingSystem As String
Public AlternateIdentifier As String
Public AlternateText As String
Public NameOfAlternateCodingSystem As String

Public Function Emit(dest As Stream)
  If Not IsNull(Identifier) And If Identifier <> "" Then
     If Not IsNull(AlternateIdentifier) And If AlternateIdentifier <> "" Then
       modHL7Emitter.Emit("^", dest, [Identifier, Text, NameOfCodingSystem, AlternateIdentifier, AlternateText, NameOfAlternateCodingSystem]) 
     Else
       modHL7Emitter.Emit("^", dest, [Identifier, Text, NameOfCodingSystem])
     Endif
  Endif 
End

