' Gambas class file

' ' Gambas class file
' CX Extended composite ID With check digit    
' 
' ID(ST) 
' check digit(ST) 
' code identifying the check digit scheme employed(ID) 
' assigning authority(HD)) 
' identifier type code( Is )
' assigning facility(HD)
' 

Public ID As String
Public CheckDigit As String
Public CodeIdentifyingTheCheckDigitSchemeEmployed As String
Public AssigningAuthority As New CHL7Datatype_HD("&")
Public IdentifierTypeCode As String
Public AssigningFacility As New CHL7Datatype_HD("&")

Public Sub Emit(dest As Stream)

  If IsNull(ID) Then Return
  If IsNull(CheckDigit) And If IsNull(IdentifierTypeCode) Then
     modHL7Emitter.Emit("^", dest, [ID])
  Else
     modHL7Emitter.Emit("^", dest, [ID, CheckDigit, CodeIdentifyingTheCheckDigitSchemeEmployed, AssigningAuthority, IdentifierTypeCode, AssigningFacility]) 
  Endif
End
