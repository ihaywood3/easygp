' Gambas class file

   ' Date YYYY[MM[DD]] 

Public mydate As Date

Private Function WithLeadingZero(i As Integer) As String
   
   If i < 10 Then 
      Return "0" & CString(i)
   Else
      Return CString(i)
   Endif
   
End



Public Sub Emit(dest As Stream)
  Print #dest, Year(mydate); WithLeadingZero(Month(mydate)); WithLeadingZero(Day(mydate));
End
