' Gambas class file

'  Used only As Part Of EI And other data types
   
Public ApplicationIdentifier As String   
Public UniversalID As String       '   Coded value for user-defined tables -> do what ever you want
Public UniversalIDType As String   '   Coded values for HL7 tables 

Public Function Emit(dest As Stream)
   
  If IsNull(UniversalID) Or If UniversalID = "" Then
     modHL7Emitter.Emit("^", dest, [ApplicationIdentifier])
  Else
     modHL7Emitter.Emit("^", dest, [ApplicationIdentifier, UniversalID, UniversalIDType])
  Endif
   
End

