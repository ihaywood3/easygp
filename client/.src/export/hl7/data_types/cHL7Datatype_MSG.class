' Gambas class file

' special field type for MSH-9 "message type"

Public message_code As String
Public trigger_event As String
Public message_structure As String

Public Sub Emit(dest As Stream)

  If IsNull(message_structure) Then
     modHL7Emitter.Emit("^", dest, [message_code, trigger_event])
  Else
     modHL7Emitter.Emit("^", dest, [message_code, trigger_event, message_structure]) 
  Endif
End
