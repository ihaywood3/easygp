' ' Gambas class file
' TS Time stamp   
'  YYYY[MM[DD[HHMM[SS[.S[S[S[S]]]]]]]][ + / - ZZZZ] ^ < degree Of precision >

Public mydate As Date

Private Function WithLeadingZero(i As Integer) As String
   
   If i < 10 Then 
      Return "0" & CString(i)
   Else
      Return CString(i)
   Endif
   
End



Public Function Emit(dest As Stream)
  Print #dest, Year(mydate); WithLeadingZero(Month(mydate)); WithLeadingZero(Day(mydate)); WithLeadingZero(Hour(mydate)); WithLeadingZero(Minute(mydate)); WithLeadingZero(Second(mydate)); "+1000";
End

