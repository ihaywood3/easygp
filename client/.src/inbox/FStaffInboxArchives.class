' Gambas class file
Private FHTML As FHtmlViewer

Public Sub Form_Open()
  
  FHTML = New FHtmlViewer(Vbox1)
    TreeView1.Add("Root", "Inbox")
     TreeView1.Add("Advertising", "Advertising")
End

Public Sub TreeView1_Menu()

  mnuStaffInbox.Popup()

End
Public Sub mnuStaffInbox_Click()
  
  Select Case Last.tag
    Case "new folder"
           TreeView1.Add("new folder", "New Folder")
           TreeView1.Editable = True
           TreeView1.ed
           
  End Select
  
End
