' Gambas class file

Private Form_Documents_imported As FDocumentsImported 

Public Sub Init()
   
   With Form_Documents_Imported = New FDocumentsImported(VBox_Documents)
      .Set_Embedded_In_Form("FResearchPathology")
      .Init()
      .VBox_RightSide_Lower.Visible = False  
      .VBox_RightSide_Lower2.Visible = True    'temp to hold some texxt
      .VSplit_RightSide.Layout[1] = 0
   End With
   ComboBox1.Add("MSU Antibiotic Sensitivites")
   ComboBox1.Index = -1
End

Public Sub ComboBox1_Click()
   
     Form_Documents_imported.txtFilterDocumentTag.Text = "msu"
     Form_Documents_imported.Reload
   
End

Public Sub Button1_Click()
   
   Print Form_Documents_Imported.VBox_RightSide_Lower.Visible
   
End
